# Production Environment Configuration
# NOTE: This file is for reference only. All actual values should be set in Vercel Dashboard.
# Next.js will load environment variables from Vercel's environment, not from this file during builds.
# This file exists only for local development reference.

NODE_ENV=production
VITE_APP_URL=https://aias-consultancy.com
LOG_LEVEL=warn

# Database - Production PostgreSQL
# These variables should be set in Vercel dashboard, not here
# DATABASE_URL=postgresql://aias_user:PASSWORD@HOST:5432/aias_production
# DIRECT_URL=postgresql://aias_user:PASSWORD@HOST:5432/aias_production

# Supabase - Production
# These variables should be set in Vercel dashboard
# VITE_SUPABASE_URL=https://PROJECT_ID.supabase.co
# VITE_SUPABASE_ANON_KEY=ANON_KEY_HERE
# SUPABASE_SERVICE_ROLE_KEY=SERVICE_ROLE_KEY_HERE

# Redis - Production
# REDIS_URL=redis://HOST:6379
# REDIS_PASSWORD=PASSWORD_HERE

# Stripe - Production
# VITE_STRIPE_PUBLISHABLE_KEY=PK_HERE
# STRIPE_SECRET_KEY=SK_HERE
# STRIPE_WEBHOOK_SECRET=WH_SECRET_HERE
# STRIPE_PRICE_BASIC=PRICE_ID_HERE
# STRIPE_PRICE_PRO=PRICE_ID_HERE
# STRIPE_PRICE_ENTERPRISE=PRICE_ID_HERE

# AI Providers - Production
VITE_AI_PRIMARY_PROVIDER=openai
# OPENAI_API_KEY=KEY_HERE
# ANTHROPIC_API_KEY=KEY_HERE
# GOOGLE_API_KEY=KEY_HERE

# Observability - Production
# OTEL_EXPORTER_OTLP_ENDPOINT=ENDPOINT_HERE
# SENTRY_DSN=DSN_HERE
# METRICS_TOKEN=TOKEN_HERE

# Email - Production
# RESEND_API_KEY=KEY_HERE
# POSTMARK_API_TOKEN=TOKEN_HERE

# Security - Production
# ENCRYPTION_KEY=KEY_HERE
# JWT_SECRET=SECRET_HERE
CSP_REPORT_URI=https://aias-consultancy.com/csp-report

# Feature Flags - Production
VITE_ENABLE_LEMON_SQUEEZY=true
VITE_ENABLE_SENTRY=true
VITE_ENABLE_OTEL=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_MONETIZATION=true

# Business Configuration
VITE_COMPANY_NAME=AIAS - AI Agent Solutions
VITE_COMPANY_EMAIL=hello@aias-consultancy.com
VITE_SUPPORT_EMAIL=support@aias-consultancy.com
VITE_SALES_EMAIL=sales@aias-consultancy.com

# Monetization
VITE_FREE_TRIAL_DAYS=14
VITE_REFERRAL_BONUS=100
VITE_AFFILIATE_COMMISSION=0.2

# Partnership
VITE_PARTNER_PORTAL_URL=https://partners.aias-consultancy.com
VITE_WHITELABEL_ENABLED=true
