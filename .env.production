# Production Environment Configuration
NODE_ENV=production
VITE_APP_URL=https://aias-consultancy.com
LOG_LEVEL=warn

# Database - Production PostgreSQL
DATABASE_URL=postgresql://aias_user:${DB_PASSWORD}@${DB_HOST}:5432/aias_production
DIRECT_URL=postgresql://aias_user:${DB_PASSWORD}@${DB_HOST}:5432/aias_production

# Supabase - Production
VITE_SUPABASE_URL=https://${SUPABASE_PROJECT_ID}.supabase.co
VITE_SUPABASE_ANON_KEY=${SUPABASE_ANON_KEY}
SUPABASE_SERVICE_ROLE_KEY=${SUPABASE_SERVICE_ROLE_KEY}

# Redis - Production
REDIS_URL=redis://${REDIS_HOST}:6379
REDIS_PASSWORD=${REDIS_PASSWORD}

# Stripe - Production
VITE_STRIPE_PUBLISHABLE_KEY=${STRIPE_PUBLISHABLE_KEY}
STRIPE_SECRET_KEY=${STRIPE_SECRET_KEY}
STRIPE_WEBHOOK_SECRET=${STRIPE_WEBHOOK_SECRET}
STRIPE_PRICE_BASIC=${STRIPE_PRICE_BASIC}
STRIPE_PRICE_PRO=${STRIPE_PRICE_PRO}
STRIPE_PRICE_ENTERPRISE=${STRIPE_PRICE_ENTERPRISE}

# AI Providers - Production
VITE_AI_PRIMARY_PROVIDER=openai
OPENAI_API_KEY=${OPENAI_API_KEY}
ANTHROPIC_API_KEY=${ANTHROPIC_API_KEY}
GOOGLE_API_KEY=${GOOGLE_API_KEY}

# Observability - Production
OTEL_EXPORTER_OTLP_ENDPOINT=${OTEL_ENDPOINT}
SENTRY_DSN=${SENTRY_DSN}
METRICS_TOKEN=${METRICS_TOKEN}

# Email - Production
RESEND_API_KEY=${RESEND_API_KEY}
POSTMARK_API_TOKEN=${POSTMARK_API_TOKEN}

# Security - Production
ENCRYPTION_KEY=${ENCRYPTION_KEY}
JWT_SECRET=${JWT_SECRET}
CSP_REPORT_URI=https://aias-consultancy.com/csp-report

# Feature Flags - Production
VITE_ENABLE_LEMON_SQUEEZY=true
VITE_ENABLE_SENTRY=true
VITE_ENABLE_OTEL=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_MONETIZATION=true

# Business Configuration
VITE_COMPANY_NAME=AIAS - AI Agent Solutions
VITE_COMPANY_EMAIL=hello@aias-consultancy.com
VITE_SUPPORT_EMAIL=support@aias-consultancy.com
VITE_SALES_EMAIL=sales@aias-consultancy.com

# Monetization
VITE_FREE_TRIAL_DAYS=14
VITE_REFERRAL_BONUS=100
VITE_AFFILIATE_COMMISSION=0.2

# Partnership
VITE_PARTNER_PORTAL_URL=https://partners.aias-consultancy.com
VITE_WHITELABEL_ENABLED=true